package loopDemo;

public class Main {

	public static void main(String[] args) {
		
		// For Döngüsü
		for(int i=1; i<=10; i+=2) 
		{
			System.out.println(i);
		}
		System.out.println("For Döngüsü Bitti");
		
		int i=2;
		//While Dögüsü
		int a = 1;
		while(a <= 10) {
			System.out.println(a);
			a++;
		}
		System.out.println("While Döngüsü Bitti");
	}

}
