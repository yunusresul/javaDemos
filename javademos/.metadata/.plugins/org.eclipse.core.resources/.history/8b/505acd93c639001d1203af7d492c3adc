package loopDemo;

public class Main {

	public static void main(String[] args) {
		
		// For Döngüsü
		for(int i=1; i<=10; i+=2) 
		{
			System.out.println(i);
		}
		System.out.println("For Döngüsü Bitti");
		
		int i=2;
		//While Dögüsü
		while(i<=10)
		{
			i+=2;
			System.out.println(i);
			
		}
		System.out.println("For Döngüsü Bitti");
	}

}
