package recapDemo1;

public class ReCap {

	public static void main(String[] args) {
		int sayi1 = 30, sayi2 = 25, sayi3 = 20, enBuyuk;
		enBuyuk = sayi1;
		
		if (enBuyuk<sayi2) {
			enBuyuk=sayi2;
		}
		if (enBuyuk<sayi3) {
			enBuyuk=sayi3;
		}
		System.out.println("En büyük sayı = " + enBuyuk);
	}

}
