package multiDimensionalArrayDemo;

public class Main {

	public static void main(String[] args) {
		String[][] sehirler = new String[3][3];
		sehirler[0][0]="Ä°stanbul";
		sehirler[0][1]="Bursa";
		sehirler[0][2]="Yalova";
		sehirler[1][0]="Ankara";
		sehirler[1][1]="Konya";
		sehirler[1][2]="Kayseri";
		sehirler[2][0]="Samsun";
		sehirler[2][1]="Ordu";
		sehirler[2][2]="Rize";
		
		
		for(int i=0; i<=2; i++) {
			for(int j=0; j<=2; i++) {
				System.out.println(sehirler[i][j]);
			}
		}

	}

}
