package loopDemo;

public class Main {

	public static void main(String[] args) {
		
		// For Döngüsü
		for(int i=1; i<=10; i+=2) 
		{
			System.out.println(i);
		}
		System.out.println("For Döngüsü Bitti");
		
		
		//While Dögüsü
		int i = 2;
		while(i <= 10) {
			System.out.println(i);
			i+=2;
		}
		System.out.println("While Döngüsü Bitti");
	}

}
